# Load things
[[ -f "$HOME/.aliases" ]] && source "$HOME/.aliases"
[[ -f "$HOME/.initializers" ]] && source "$HOME/.initializers"
[[ -f "$HOME/.functions" ]] && source "$HOME/.functions"

# Include our scripts into $PATH
[[ -d "$HOME/.scripts" ]] && export PATH=$HOME/.scripts:$PATH

# Settings
export EDITOR=vim